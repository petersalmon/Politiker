      <div id="contact-history-container">
        <h3>Contact History</h3>
        <div id="contact-history-container" class="sub-container">
        </div>
      </div>
      <div id="support-prob-container">
        <h3>Level of Support</h3>
        <div id="support-prob-sub-container" class="sub-container"></div>
      </div>
      <div id="button-container">
        <h3>Add to Campaign List</h3>
        <select id="campaign-select">
          <option disabled selected value> Select a Campaign ... </option>
        </select>
        <div id="button-sub-container" class="sub-container">
          <button class="btn btn-primary">Add to Phone List</button>
          <button class="btn btn-primary">Add to Text List</button>
          <button class="btn btn-primary">Add to Mailing List</button>
        </div>
      </div>
    </div>

    <!-- JQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
