    <form id="voter-id-search" class="form-inline d-flex justify-content-center md-form form-sm mt-0" action="/Politiker/voter-profiles/search/" method="GET">
      <i class="fas fa-search fa-lg" aria-hidden="true"></i>
      <input class="form-control form-control-lg ml-3 w-75" type="text" placeholder="Search Voter ID ..."
        aria-label="Search" name="voterid" value=<%= voterData[0].ID %>>
    </form> 

    <div id="main-container">
      <div id="contact-info-container">
        <h3>Contact Info</h3>
        <div id="contact-info-sub-container" class="sub-container">
          <p><b>Full Name:</b> <%= voterData[0].FirstName %> <%= voterData[0].LastName %> </p>
          <p><b>Street Address:</b> <%= voterData[0].StreetName %></p>
          <p><b>City:</b> <%= voterData[0].City %></p>
          <p><b>Zip Code:</b> <%= voterData[0].ZipCode %></p>
          <p><b>Phone Number:</b> <%= voterData[0].TelephoneNumber %></p>
        </div>
      </div>
      <div id="voter-info-container">
        <h3>Voter Info</h3>
        <div id="voter-info-sub-container" class="sub-container">
          <p><b>Date of Birth:</b> <%= voterData[0].DOB %></p>
          <p><b>Gender:</b> <%= voterData[0].Gender %></p>
          <p><b>Political Affiliation:</b> <%= voterData[0].PoliticalParty %></p>
          <p><b>Registration Date:</b> <%= voterData[0].RegistrationDate %></p>
          <p><b>Year Last Voted:</b> <%= voterData[0].YearLastVoted %></p>
          <p><b>Voter Type:</b> <%= voterData[0].VoterType %></p>
          <p><b>StatusCode:</b> <%= voterData[0].StatusCode %></p>
        </div>
      </div>
      <div id="districts-container">
        <h3>Districts</h3>
        <div id="districts-sub-container" class="sub-container">
          <p><b>Assembly District:</b> <%= voterData[0].AssemblyDistrict %></p>
          <p><b>Civil Court District:</b> <%= voterData[0].CivilCourtDistrict %></p>
          <p><b>Congressional District:</b> <%= voterData[0].CongressDistrict %></p>
          <p><b>Council District:</b> <%= voterData[0].CouncilDistrict %></p>
          <p><b>Election District:</b> <%= voterData[0].ElectionDistrict %></p>
          <p><b>Judicial District:</b> <%= voterData[0].JudicialDistrict %></p>
          <p><b>Senate District:</b> <%= voterData[0].SenateDistrict %></p>
        </div>
      </div>
